 label=\"订单价格\" prop=\"order_price\"></el-table-column>\r\n        <el-table-column label=\"是否付款\" prop=\"pay_status\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag type=\"primary\" v-if=\"scope.row.pay_status === '0'\"\r\n              >未付款</el-tag\r\n            >\r\n            <el-tag type=\"danger\" v-else>已付款</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"是否发货\" prop=\"is_send\"></el-table-column>\r\n        <!-- 下单时间 -->\r\n        <el-table-column label=\"下单时间\" prop=\"create_time\">\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.create_time | dateFormat }}</template\r\n          >\r\n        </el-table-column>\r\n        <!-- 操作 -->\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- 修改按钮 -->\r\n            <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              icon=\"el-icon-edit\"\r\n              @click=\"showBox\"\r\n            ></el-button>\r\n            <!-- 物流进度按钮 -->\r\n            <el-button\r\n              type=\"success\"\r\n              size=\"mini\"\r\n              icon=\"el-icon-location\"\r\n              @click=\"showProgressBox\"\r\n            ></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[3, 5, 7, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        background=\"\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 修改地址的对话框 -->\r\n    <el-dialog\r\n      title=\"修改地址\"\r\n      :visible.sync=\"addressVisible\"\r\n      width=\"50%\"\r\n      @close=\"addressDialogClosed\"\r\n    >\r\n      <el-form\r\n        :model=\"addressForm\"\r\n        :rules=\"addressFormRules\"\r\n        ref=\"addressFormRef\"\r\n        label-width=\"100px\"\r\n      >\r\n        <el-form-item label=\"省市区/县\" prop=\"address1\">\r\n          <el-cascader\r\n            expand-trigger=\"hover\"\r\n            v-model=\"addressForm.address1\"\r\n            :options=\"cityData\"\r\n          ></el-cascader>\r\n        </el-form-item>\r\n        <el-form-item label=\"详细地址\" prop=\"address2\">\r\n          <el-input v-model=\"addressForm.address2\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addressVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addressVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- 物流进度的对话框 -->\r\n    <el-dialog title=\"物流进度\" :visible.sync=\"progressVisible\" width=\"50%\">\r\n      <span>这是一段信息</span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport cityData from \"./citydata\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      queryInfo: {\r\n        query: \"\",\r\n        pagesize: 3,\r\n        pagenum: 1,\r\n      },\r\n      orderlist: [],\r\n      total: 0,\r\n      addressVisible: false,\r\n      addressForm: {},\r\n      addressFormRules: {\r\n        address1: [\r\n          { required: true, message: \"请输入省市区/县\", trigger: \"blur\" },\r\n        ],\r\n        address2: [\r\n          { required: true, message: \"请填写详细地址\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n      cityData,\r\n      progressVisible: false,\r\n      progreInfo:''\r\n    };\r\n  },\r\n  created() {\r\n    this.getOrderList();\r\n  },\r\n  methods: {\r\n    async getOrderList() {\r\n      const { data: res } = await this.$http.get(\"orders\", {\r\n        params: this.queryInfo,\r\n      });\r\n      if (res.meta.status !== 200)\r\n        return this.$message.error(\"获取订单列表失败\");\r\n      this.orderlist = res.data.goods;\r\n      this.total = res.data.total;\r\n    //   console.log(this.orderlist);\r\n    },\r\n    handleSizeChange(newSize) {\r\n      this.queryInfo.pagesize = newSize;\r\n      this.getOrderList();\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pagenum = newPage;\r\n      this.getOrderList();\r\n    },\r\n    // 展示地址的对话框\r\n    showBox() {\r\n      this.addressVisible = true;\r\n    },\r\n    addressDialogClosed() {\r\n      this.$refs.addressFormRef.resetFields();\r\n    },\r\n    async showProgressBox() {\r\n        const{data:res}=await this.$http.get('kuaidi/1106975712662')\r\n        if(res.meta.status!==200){\r\n            return this.$message.error('获取物流信息失败')\r\n        }\r\n     console.log(res.data);\r\n        // this.progreInfo = res.data\r\n        this.progressVisible = true\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components/order"}]}